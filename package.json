{
  "name": "tic-tac-toe",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "dev": "vite client",
    "dev:all": "concurrently \"npm run dev\" \"npm run server:dev\"",
    "build": "tsc -p client/tsconfig.json && vite build client && tsc -p server/tsconfig.json",
    "start": "node server/dist/server/src/index.js",
    "server:dev": "tsx watch server/src/index.ts",
    "preview": "vite preview client"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/node": "^22.10.2",
    "concurrently": "^9.1.2",
    "tsx": "^4.19.2",
    "typescript": "~5.9.3",
    "vite": "^7.2.4"
  },
  "dependencies": {
    "express": "^5.0.1",
    "phaser": "^3.90.0",
    "rate-limiter-flexible": "^9.0.1",
    "socket.io": "^4.8.3",
    "socket.io-client": "^4.8.3"
  }
}